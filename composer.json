{
    "name": "ekaty/restaurant-agent",
    "description": "Autonomous agent for maintaining eKaty restaurant database with Google Places API",
    "type": "project",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "monolog/monolog": "^3.5",
        "symfony/console": "^6.4",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "EkatyAgent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EkatyAgent\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "sync": "php bin/agent sync",
        "test": "phpunit"
    }
}
